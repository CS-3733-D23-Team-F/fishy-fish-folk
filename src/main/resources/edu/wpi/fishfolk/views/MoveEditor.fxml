               </HBox>
               <HBox prefHeight="76.0" prefWidth="396.0">
                  <children>
                     <VBox prefHeight="76.0" prefWidth="175.0" />
                     <MFXButton fx:id="submitmove" prefHeight="40.0" prefWidth="80.0" style="-fx-background-color: #012D5A; -fx-text-fill: #f1f1f1;" text="Submit">
                        <HBox.margin>
                           <Insets top="5.0" />
                        </HBox.margin></MFXButton>
                     <VBox prefHeight="76.0" prefWidth="37.0" />
                     <MFXButton fx:id="clearMove" prefHeight="40.0" prefWidth="80.0" style="-fx-background-color: #012D5A; -fx-text-fill: #f1f1f1;" text="Clear">
