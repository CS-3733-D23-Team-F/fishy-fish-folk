            <children>
               <TableView fx:id="locationtable" editable="true" prefHeight="820.0" prefWidth="412.0" tableMenuButtonVisible="true">
                  <columns>
                     <TableColumn fx:id="locationlongname" prefWidth="203.0" text="Long Name" />
                     <TableColumn fx:id="shortname" prefWidth="144.0" text="Short Name" />
                     <TableColumn fx:id="type" prefWidth="85.0" text="Type" />
                  </columns>
               </TableView>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Add a new Location:">
                  <font>
                     <Font size="16.0" />
